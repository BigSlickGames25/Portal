{
  "name": "portal-library-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently -n server,client -c cyan,magenta \"npm run dev -w server\" \"npm run dev -w client\"",
    "seed": "npm run seed -w server",
    "reset": "npm run reset -w server"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
